{
  "name": "geo-trees",
  "version": "0.0.21",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "serve": "vite preview",
    "start": "node .output/server/index.mjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "vitest run --coverage && playwright test",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "lint": "biome lint && node scripts/check-file-size.js && node scripts/check-file-naming.js",
    "lint:fix": "biome lint --write --unsafe .",
    "check": "biome check && node scripts/check-file-size.js && node scripts/check-file-naming.js",
    "check-file-size": "node scripts/check-file-size.js",
    "check-file-naming": "node scripts/check-file-naming.js",
    "find-unused-files": "node scripts/find-unused-files.js",
    "typecheck": "tsc --noEmit",
    "generate-api": "NODE_TLS_REJECT_UNAUTHORIZED=0 openapi-ts --file hey-api.config.ts",
    "docker:build": "docker build -t geo-trees .",
    "docker:run": "docker run -p 3000:3000 geo-trees",
    "docker:up": "docker build -t geo-trees . && docker run -p 3000:3000 geo-trees",
    "docker:compose:up": "docker-compose up --build",
    "docker:compose:down": "docker-compose down"
  },
